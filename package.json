{
  "name": "patron-next-typescipt-pages",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint --ext .ts,.tsx ./src",
    "lint:check": "yarn eslint",
    "lint:fix": "yarn eslint --fix",
    "lint:staged": "eslint --fix",
    "prettier": "prettier \"**/*.+(json|ts|tsx|css|md)\"",
    "format:check": "yarn prettier --check",
    "format:fix": "yarn prettier --write",
    "format:staged": "prettier --write",
    "types:check": "tsc --noEmit --pretty",
    "types:staged": "tsc --noEmit --pretty",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:staged": "jest --bail --findRelatedTests --passWithNoTests",
    "e2e:open": "./node_modules/.bin/cypress open",
    "e2e:run": "./node_modules/.bin/cypress run",
    "e2e:run:headed": "./node_modules/.bin/cypress run --headed",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.5",
    "@ant-design/icons": "^5.0.1",
    "@ant-design/static-style-extract": "^1.0.1",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/node": "20.0.0",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "@types/uuid": "^9.0.1",
    "antd": "^5.4.6",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "csstype": "^3.1.2",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.0",
    "next": "13.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.5",
    "typescript": "5.0.4",
    "uuid": "^9.0.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/nextjs": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react-test-renderer": "^18.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "local-cypress": "^1.2.6",
    "next-router-mock": "^0.9.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "storybook": "^7.0.9",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0"
  }
}
